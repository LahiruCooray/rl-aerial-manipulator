<?xml version="1.0" ?>
<sdf version="1.9">
  <model name="f550">
    <static>false</static>
    <pose>0 0 0 0 0 0</pose>

<!-- ######################################## LOWER_PLATE # ######################################## -->
    <link name="base_link">
	<gravity>true</gravity>
	<pose>0 0 0.0015 0 0 0</pose>
	
      <inertial>
  <pose>0.00 0 -0.00 0 -0 0</pose>
        <mass>0.6</mass>
        <inertia>
          <ixx>0.011</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.015</iyy>
          <iyz>0</iyz>
          <izz>0.021</izz>
        </inertia>
      </inertial>

      <collision name="base_link_collision">
        <geometry>
          <cylinder>
            <radius>0.15</radius>
            <length>0.003</length>
          </cylinder>
        </geometry>
      </collision>

      <visual name="base_link_visual">
        <geometry>
          <mesh>
            <scale>0.001 0.001 0.001</scale>
            <uri>file://f550/meshes/Lower_plate_CG.STL</uri>
          </mesh>
        </geometry>
        <material>  
          <ambient>0.3 0.3 0.3 1</ambient>
          <diffuse>0.5 0.5 0.5 1</diffuse>
          <specular>0.8 0.8 0.8 1</specular>
        </material>
      </visual>

      <!-- IMU Sensor (modeled after ICM-42688-P) -->
      <sensor name="imu_sensor" type="imu">
        <pose>0 0 0.02 0 0 0</pose>
        <always_on>1</always_on>
        <update_rate>250</update_rate>
        <imu>
          <angular_velocity>
            <x>
              <noise type="gaussian">
                <mean>0</mean>
                <stddev>0.0003394</stddev>
                <dynamic_bias_stddev>3.8785e-05</dynamic_bias_stddev>
                <dynamic_bias_correlation_time>1000</dynamic_bias_correlation_time>
              </noise>
            </x>
            <y>
              <noise type="gaussian">
                <mean>0</mean>
                <stddev>0.0003394</stddev>
                <dynamic_bias_stddev>3.8785e-05</dynamic_bias_stddev>
                <dynamic_bias_correlation_time>1000</dynamic_bias_correlation_time>
              </noise>
            </y>
            <z>
              <noise type="gaussian">
                <mean>0</mean>
                <stddev>0.0003394</stddev>
                <dynamic_bias_stddev>3.8785e-05</dynamic_bias_stddev>
                <dynamic_bias_correlation_time>1000</dynamic_bias_correlation_time>
              </noise>
            </z>
          </angular_velocity>
          <linear_acceleration>
            <x>
              <noise type="gaussian">
                <mean>0</mean>
                <stddev>0.004</stddev>
                <dynamic_bias_stddev>0.006</dynamic_bias_stddev>
                <dynamic_bias_correlation_time>300</dynamic_bias_correlation_time>
              </noise>
            </x>
            <y>
              <noise type="gaussian">
                <mean>0</mean>
                <stddev>0.004</stddev>
                <dynamic_bias_stddev>0.006</dynamic_bias_stddev>
                <dynamic_bias_correlation_time>300</dynamic_bias_correlation_time>
              </noise>
            </y>
            <z>
              <noise type="gaussian">
                <mean>0</mean>
                <stddev>0.004</stddev>
                <dynamic_bias_stddev>0.006</dynamic_bias_stddev>
                <dynamic_bias_correlation_time>300</dynamic_bias_correlation_time>
              </noise>
            </z>
          </linear_acceleration>
        </imu>
      </sensor>

      <!-- Air Pressure Sensor (Barometer, modeled after BMP390) -->
      <sensor name="air_pressure_sensor" type="air_pressure">
        <always_on>1</always_on>
        <update_rate>50</update_rate>
        <air_pressure>
          <pressure>
            <noise type="gaussian">
              <mean>0</mean>
              <stddev>0.01</stddev>
            </noise>
          </pressure>
        </air_pressure>
      </sensor>

      <!-- GPS/NavSat Sensor (simplified for compatibility) -->
      <sensor name="navsat_sensor" type="navsat">
        <pose>0 0 0.03 0 0 0</pose>
        <always_on>1</always_on>
        <update_rate>30</update_rate>
      </sensor>

      <!-- Magnetometer Sensor (modeled after IIS2MDC) -->
      <sensor name="magnetometer_sensor" type="magnetometer">
        <pose>0 0 0.02 0 0 0</pose>
        <always_on>1</always_on>
        <update_rate>100</update_rate>
        <magnetometer>
          <!-- 3mgauss RMS: NOTE: noise is in tesla but sensor reports data in gauss -->
          <x>
            <noise type="gaussian">
              <stddev>0.0001</stddev>
            </noise>
          </x>
          <y>
            <noise type="gaussian">
              <stddev>0.0001</stddev>
            </noise>
          </y>
          <z>
            <noise type="gaussian">
              <stddev>0.0001</stddev>
            </noise>
          </z>
        </magnetometer>
      </sensor>      <!-- RGB Camera Sensor at ~15 Hz -->
      <!-- <sensor name="camera_sensor" type="camera">
        <pose>0.1 0 0.05 0 0 0</pose>
        <always_on>true</always_on>
        <update_rate>15</update_rate>
        <camera>
          <horizontal_fov>1.047</horizontal_fov>
          <image>
            <width>640</width>
            <height>480</height>
            <format>R8G8B8</format>
          </image>
          <clip>
            <near>0.1</near>
            <far>100</far>
          </clip>
          <noise>
            <type>gaussian</type>
            <mean>0.0</mean>
            <stddev>0.007</stddev>
          </noise>
        </camera>
      </sensor> -->
    </link>
<!-- ####################################### /LOWER_PLATE # ######################################## -->


