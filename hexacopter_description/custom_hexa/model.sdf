    <inertial>
      <mass>0.072</mass>
      <inertia>
        <ixx>8.928e-06</ixx>
        <ixy>0</ixy>
        <ixz>0</ixz>
        <iyy>8.928e-06</iyy>
        <iyz>0</iyz>
        <izz>7.056e-06</izz>
