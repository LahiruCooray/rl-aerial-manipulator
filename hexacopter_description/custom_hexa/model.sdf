<link name='rotor_0'>
  <pose>-0.255691 0.1475 0.093 0 0 2.09439510239</pose>
      <inertial>
        <mass>0.008</mass>
        <inertia>
          <ixx>5.205e-05</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
