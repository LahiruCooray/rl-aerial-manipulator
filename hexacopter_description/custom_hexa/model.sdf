        <geometry>
          <cylinder>
            <length>0.005</length>
            <radius>0.128</radius>
          </cylinder>
        </geometry>
        <surface>
          <contact>
            <collide_bitmask>0x00</collide_bitmask>
            <ode/>
          </contact>
          <friction>
            <ode/>
          </friction>
        </surface>
      </collision>

      <visual name='rotor_0_visual'>
        <geometry>
          <mesh>
            <scale>0.001 0.001 0.001</scale>
            <uri>file://custom_hexa/meshes/prop_cw_CG.stl</uri>
          </mesh>
        </geometry>
        <material>  
          <ambient>1.0 1.0 0.0 1</ambient>
