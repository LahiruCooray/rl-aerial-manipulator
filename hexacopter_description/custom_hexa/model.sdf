          <specular>0.8 0.8 0.0 1</specular>
        </material>
      </visual>
    </link>

    <joint name='rotor_0_joint' type='revolute'>
      <child>rotor_0</child>
      <parent>base_link</parent>
      <axis>
  <xyz expressed_in="__model__">0 0 1</xyz>
        <limit>
          <lower>-1e+16</lower>
          <upper>1e+16</upper>
          <!-- Increased effort limit to avoid artificial torque saturation during spin-up/control -->
          <effort>100000</effort>
          <!-- Set a large positive velocity limit to avoid artificial clamping -->
          <velocity>4000</velocity>
        </limit>
        <dynamics>
          <damping>0.005</damping>
        </dynamics>
      </axis>
      <physics>
        <ode>
          <implicit_spring_damper>1</implicit_spring_damper>
        </ode>
      </physics>
    </joint>
